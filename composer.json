{
  "name": "YourName/Boilerplate",
  "description": "Laravel PHP API Facade/Wrapper Boilerplate",
  "keywords": ["laravel", "api", "boilerplate"],
  "homepage": "",
  "license": "MIT",
  "authors": [
    {
      "name": "Your Name Here",
      "email": "Your Email Here"
    }
  ],
  "require": {
    "illuminate/support": "^6.0|^7.0|^8.0",
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.0|^7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "scrutinizer/ocular": "^1.1",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "YourName\\Boilerplate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YourName\\Boilerplate\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "YourName\\Boilerplate\\BoilerplateServiceProvider"
      ],
      "aliases": {
        "Boilerplate": "YourName\\Boilerplate\\Facades\\Boilerplate"
      }
    }
  }
}
